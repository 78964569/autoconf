<body>
  #set(User currentUser=principal())
<nav class="navbar navbar-inverse navbar-static-top" role="navigation"><!--导航-->
    <div class="container-fluid">
        <div class="navbar-header"><a class="navbar-brand" href="${CONTEXT_PATH}/">配置管理系统</a></div>

        <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-left">
                <li><a href="${CONTEXT_PATH}/">首页</a></li>
                <li><a href="${CONTEXT_PATH}/history">修改历史</a></li>
                <li><a href="${CONTEXT_PATH}/create/config/">新建配置</a></li>
                <li><a href="${CONTEXT_PATH}/replace/form/">批量修改</a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li><a href="/login/profile/">$!{currentUser.name}</a></li>
            </ul>
        </div>
    </div>
</nav>

<div class="container-fluid">
    <div class="row">
      #include("alert.inc.jetx")
        #tag layout_block_default("bodyContent")#end
    </div>
</div>
<script src="${CONTEXT_PATH}/static/datatables.min.js"></script>
#tag layout_block_default("scriptContent")#end
</body>
</html>
